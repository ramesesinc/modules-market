		<field name="billgroupid" />
		<field name="year" />
		<field name="month" />
		<field name="fromdate" />
		<field name="todate" />
		<field name="billdate" />
		<field name="discdate" />
		<field name="duedate" />
		<field name="monthname" expr="MONTH_ARRAY(month)" caption="From Month Name"/>
	</element>
</schema>
