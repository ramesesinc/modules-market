<workunit extends="com/rameses/seti2/templates/CrudForm.xml" schemaName="market_utility">
    <invokers>
        <invoker type="market_account:form:formActions" 
                 caption="Add Electricity" action="createNew" target="popup" 
                 visibleWhen="#{entity.electricity?.objid == null &amp;&amp; mode != 'create'}"
                 utilitytype="ELECTRICITY" />
        
        <invoker type="market_electricity:create" 
                 caption="Add Next Entry" action="addNextEntry" target="popup"
                 utilitytype="ELECTRICITY" />
        
        <invoker type="market_electricity:open" caption="Market Electricity" 
                 utilitytype="ELECTRICITY"
                 action="open"  target="popup" />

        <invoker type="market_electricity:removeEntity" action="removeEntity" target="process" />
    </invokers>
    <code class="com.rameses.gov.etracs.market.models.MarketUtilityModel" />
    <pages>
        <page template="com.rameses.gov.etracs.market.views.MarketUtilityPage" />
    </pages>    
</workunit>