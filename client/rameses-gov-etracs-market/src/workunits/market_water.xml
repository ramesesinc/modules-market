<workunit extends="com/rameses/seti2/templates/CrudForm.xml" schemaName="market_utility">
    <invokers>
        <invoker type="market_account:form:formActions" 
                 caption="Add Water" action="createNew" target="popup" 
                 visibleWhen="#{entity.water?.objid == null &amp;&amp; mode != 'create'}"
                 utilitytype="WATER" index="20"/>
        
        <invoker type="market_water:create" 
                 caption="Add Next Entry" action="addNextEntry" target="popup"
                 utilitytype="WATER" />
        
        <invoker type="market_water:open" caption="Market Water" 
                 utilitytype="WATER"
                 action="open"  target="popup" />
    </invokers>
    <code class="com.rameses.gov.etracs.market.models.MarketUtilityModel" />
    <pages>
        <page template="com.rameses.gov.etracs.market.views.MarketUtilityPage" />
    </pages>    
</workunit>